@page
@model RimuoviDaBraniModel
@{
    ViewData["Title"] = "Elimina brano";
}

<style>
    body {
        font-family: Montserrat;
    }
</style>



<h1>@ViewData["Title"]</h1>

<form method="post">

    <br>
    <h4> Confermi di voler eliminare questo brano?</h4>
    <br>
    <button type="submit" class="btn btn-dark mt-4">Elimina</button>
    <a asp-asp-page="Index" class="btn btn-dark mt-4">Annulla</a>

</form>