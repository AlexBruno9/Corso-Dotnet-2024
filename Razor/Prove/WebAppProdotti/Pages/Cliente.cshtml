@page
@model ClienteModel
@{
    ViewData["Title"] = "Clienti";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Nome</th>
            <th>Età</th>
            <th>n°acquisti</th>
            <th>Codice Fiscale</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var cliente in Model.Clienti)

        {
            <tr>
                <td>@cliente.Nome</td>
                <td>@cliente.Eta</td>
                <td>@cliente.N_Acquisti</td>
                <td>@cliente.CF</td>
            </tr>
        }
    </tbody>
    </table>