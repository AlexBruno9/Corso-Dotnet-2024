@model BranoViewModel
@{
    ViewData["Title"] = "Elimina brani";
}


<div class="text-light">
    <h1>@ViewData["Title"]</h1>
    <form method="post">

        <br>
        <h4> Confermi di voler eliminare questi brani?</h4>
        <br>
        <button type="submit" class="btn btn-dark border-light mt-4">Elimina</button>
        <a asp-controller="Home" asp-action="Brani" class="btn btn-dark border-light text-light mt-4">Annulla</a>

    </form>
</div>
